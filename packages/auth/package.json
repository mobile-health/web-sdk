    "eslint": "^9.34.0",
    "typescript": "5.9.2",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "@web-sdk/shared": "*",
    "@tanstack/react-query": "^5.90.2"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^13.2.0",
    "axios": "^1.12.2",
    "ua-parser-js": "^2.0.5"
  },
