{
  "name": "@web-sdk/auth",
  "version": "0.0.7",
  "type": "module",
  "description": "Auth utilities and constants for web-sdk projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobile-health/web-sdk.git",
    "directory": "packages/auth"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
